{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch CSE Motors",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/server.js",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "pnpm",
